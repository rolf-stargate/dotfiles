if [ -z "$1" ]; then
  echo "Usage: ansi2txt_extended <file>
fi

ansi2txt "$1" | sed 's/^.*echo ##\(.*\)$/################################################################################\n#### \1\n################################################################################/'| tr -d "\r" |  sed 's/^.*\x08/$ /' | sed 's/^% *\[rolf/[rolf/'
